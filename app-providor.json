{
  "expo": {
    "name": "PetMe Provider",
    "slug": "PetMeProvider",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "petmeprovider",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.petmeprovidor.app",
      "config": {
        "googleMapsApiKey": "AIzaSyCd0luF6FXBYOvJy1ZWpr3ALjjYhJ5LtOo"
      },
      "supportsRTL": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.petmeprovidor.app",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCd0luF6FXBYOvJy1ZWpr3ALjjYhJ5LtOo"
        }
      },
      "supportsRTL": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
